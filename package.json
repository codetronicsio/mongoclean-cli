{
  "name": "mongoclean-cli",
  "version": "0.0.5",
  "description": "A KISS cli to clean a mongodb",
  "main": "mongoclean-cli.js",
  "bin": {
    "mongoclean-cli": "./mongoclean-cli.js"
  },
  "scripts": {
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
