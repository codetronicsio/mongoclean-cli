    "clean",
    "test"
  ],
  "author": "codetronics",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codetronicsio/mongoclean-cli/issues"
  },
  "homepage": "https://github.com/codetronicsio/mongoclean-cli#readme",
  "dependencies": {
    "cli": "^1.0.1",
    "dotenv": "^4.0.0",
    "mongo-clean": "^1.2.2",
    "mongodb": "^2.2.28"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
